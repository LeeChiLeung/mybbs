<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
      
      <Appenders>
           <Console name="console" target="SYSTEM_OUT" />
           <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} %p - %m%n"  />      
      </Appenders>
      
      <Loggers>
         <AsyncRoot level="info" includeLocation="false">
            <AppenderRef ref="console"/>
         
         </AsyncRoot>
      
      </Loggers>
      
      

</Configuration>